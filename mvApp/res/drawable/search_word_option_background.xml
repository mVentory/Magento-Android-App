<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true"><shape android:dither="true">
            <solid android:color="#92d794" />

            <corners android:radius="4dp" />

            <stroke android:width="1dp" android:color="#75b276" />

            <padding android:bottom="5dp" android:left="10dp" android:right="10dp" android:top="5dp" />
        </shape></item>
    <item><shape android:dither="true">
            <solid android:color="#b3fbfbfb" />

            <corners android:radius="4dp" />

            <stroke android:width="1dp" android:color="#8e8e8e" />

            <padding android:bottom="5dp" android:left="10dp" android:right="10dp" android:top="5dp" />
        </shape></item>

</selector>